<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csupervise.modules.system.mapper.SysDepartMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.csupervise.modules.system.entity.SysDepart">
    <result column="id" property="id" />
    <result column="create_by" property="createBy" />
    <result column="create_time" property="createTime" />
    <result column="update_by" property="updateBy" />
    <result column="update_time" property="updateTime" />
        <result column="parent_id" property="parentId" />
        <result column="depart_name" property="departName" />
        <result column="depart_order" property="departOrder" />
        <result column="org_category" property="orgCategory" />
        <result column="org_type" property="orgType" />
        <result column="org_code" property="orgCode" />
        <result column="del_flag" property="delFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        create_by,
        create_time,
        update_by,
        update_time,
        parent_id, depart_name, depart_order, org_category, org_type, org_code, del_flag
    </sql>

</mapper>
